NAME = condor_had condor_replica_up condor_replica_down
all_target($(NAME))

CFLAGS = $(STD_C_FLAGS) 
C_PLUS_FLAGS =  $(STD_C_PLUS_FLAGS)
LIB = $(DAEMONCORE_LIB) $(STD_LIBS)

#OBJ =  ReplicaVersion.o Replication.o StateMachine.o Listener.o HAD.o
#OBJ =  StateMachine.o Listener.o HAD.o
OBJ =  ReplicaVersion.o Replication.o StateMachine.o HAD.o

TOBJ =  Transfer.o

c_plus_target(condor_had,$(OBJ),$(LIB))
release_target(condor_had,sbin,EXECUTABLE_MODE)
strip_target(condor_had,sbin,EXECUTABLE_MODE)
static_target(condor_had,$(OBJ),$(LIB),$(STATIC_DIR)/sbin)
pure_c_plus_target(condor_had.pure,$(OBJ),$(LIB))
purecov_c_plus_target(condor_had.pc,$(OBJ),$(LIB))

c_plus_target(condor_replica_up,$(TOBJ),$(LIB))
release_target(condor_replica_up,bin,EXECUTABLE_MODE)
strip_target(condor_replica_up,bin,EXECUTABLE_MODE)
static_target(condor_replica_up,$(TOBJ),$(LIB),$(STATIC_DIR)/bin)

